{{#each this}}
<li>
  <article>
    <div>
      <h2>{{name}}</h2>
      <p>Capital: {{capital}}</p>
      <p>Population: {{population}}</p>
      <p>Languages:</p>
      <ol>
        {{#each languages}}
        <li>{{this.name}}</li>
        {{/each}}
      </ol>
    </div>
    <img src="{{flag}}" alt="flag" width="480" />
  </article>
</li>
{{/each}}